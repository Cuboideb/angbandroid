<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/frameTermOverlay"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameTermNoOverlayBottom"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@id/frameBL"
        android:layout_toStartOf="@id/frameBR"
        android:layout_above="@+id/frameBC">
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameTermNoOverlayTop"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@id/frameTL"
        android:layout_toStartOf="@id/frameTR"
        android:layout_above="@+id/frameBC">
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameTL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameBL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true">
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameTR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true">
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameBR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true">
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameBC"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true">
    </FrameLayout>
</RelativeLayout>
